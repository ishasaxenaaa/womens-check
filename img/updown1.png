    function facebookcheckoutlogin(data){
        var fbregisterUrl = $("#fb-register-url").attr("data-url");
        var CSRFToken=$("#ajaxCSRF").val();
        var fbProfiledata={
            'id':data.id,
            'first_name':data.first_name,
            'last_name':data.last_name,
            'email':data.email,
            'picture':data.picture.data.url,
            'gender' : data.gender
        }

        $.ajax({
            type: "POST",              
            url: fbregisterUrl,
            data:"fbProfiledata="+encodeURIComponent(JSON.stringify(fbProfiledata)),
            success: function (response) {
            	var redirectpath = response.replace("redirect:", "");
            	if(response == 'missing'){
            		$('#id_token').val(data.id);
            		$( '.member-login ,.sign-up-wrap' ).css('display','none');
            		$('.overlay').css('display','none');
            		$("#fbMissingMobile").modal('show');
            		 $('.loaderContainerGlobal').removeClass("login-loader").hide();
            		} else if (redirectpath=="/my-account") {
                if(secureLoginRedirectURL) {
                  location.href = secureLoginRedirectURL;
                }
                else {
                  if(location.href && location.href.indexOf("#login") !== -1) {
                    location.href= "/";
                  } else if(window.location.href.indexOf('/first-citizen/non-member')!= -1){   
                     window.location.href = "/p-"+$("#fccProductCode").val();
                  } else {
                    location.reload();
                  }
                }
                  window.location.hash = '';
                  trackFacebookLoginSuccessResponse();
                  trackDYLogin();
            	} else {
            		trackFacebookLoginFailureResponse();
            		$('.loaderContainerGlobal').removeClass("login-loader").hide();
            	}
            },
            error: function (xhr, status, error) {
                $('.loaderContainerGlobal').removeClass("login-loader").hide();
                alert('error occured in FBConnect.ashx');
                trackFacebookLoginFailureResponse();
            }
        });
      }
    
    function facebookdata(response){

        var request="https://graph.facebook.com/me?access_token="+response.authResponse.accessToken;
        $.ajax({
              type: "GET",
              dataType: "json", // what data type to expect from the server
              url: request,
              success: function (data) {
            	  if(data.hasOwnProperty('email')){
                 facebookcheckoutlogin(data);                  
              }else{
            	  facebookapidata(response);
              }
            	  },
              error: function (xhr, status, error) {
            	  alert(status+' occured in FBConnect. Refresh the page and try again');
            	  trackFacebookLoginFailureResponse();
              }
          });
    }
    function facebookapidata(response){
        var request="https://graph.facebook.com/me?fields=email,picture.height(961),name,first_name,last_name,gender&access_token="+response.authResponse.accessToken;
        $.ajax({
              type: "GET",
              dataType: "json", // what data type to expect from the server
              url: request,
              success: function (data) {
            	  
                 facebookcheckoutlogin(data);                  
              },
              error: function (xhr, status, error) {
            	  alert(status+' occured in FBConnect. Refresh the page and try again');
            	  trackFacebookLoginFailureResponse();
              }
          });
